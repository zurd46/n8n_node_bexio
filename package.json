{
  "name": "n8n-nodes-bexio",
  "version": "1.0.9",
  "description": "n8n node for Bexio API integration",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "bexio",
    "accounting",
    "crm"
  ],
  "license": "MIT",
  "homepage": "https://github.com/zurd46/n8n-nodes-bexio",
  "author": {
    "name": "Daniel Zurm√ºhle",
    "email": "daniel.zurmuehle@zurdai.com",
    "url": "https://zurdai.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zurd46/n8n-nodes-bexio.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/BexioApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Bexio/Bexio.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "~7.18.0",
    "eslint": "~8.57.0",
    "eslint-plugin-n8n-nodes-base": "~1.16.0",
    "gulp": "~5.0.0",
    "n8n-workflow": "~1.52.0",
    "prettier": "~3.3.0",
    "tsx": "^4.20.6",
    "typescript": "~5.6.0"
  },
  "peerDependencies": {
    "n8n-workflow": "^1.0.0"
  },
  "dependencies": {
    "n8n-core": "^1.0.0"
  }
}
